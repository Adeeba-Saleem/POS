                    xmlns:local="clr-namespace:POS.UI.Style">

    <Color x:Key="Nav1">pink</Color>
    <Color x:Key="Nav2">yellow</Color>

    <SolidColorBrush x:Key="BrushPrimary1" Color="{StaticResource Nav1}"/>
    <SolidColorBrush x:Key="BrushPrimary2" Color="{StaticResource Nav2}"/>

</ResourceDictionary>